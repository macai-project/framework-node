  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/macai-project/framework-node.git"
  },
  "author": "Giovanni Giordano <giovanni.giordano@macaiapp.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "release": "release-it",
    "format": "prettier --write 'src/**/*.ts'",
