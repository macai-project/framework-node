    "cross-fetch": "^3.1.4",
    "fp-ts": "^2.11.5",
    "graphql": "15.7.0",
    "io-ts": "^2.2.16",
    "mysql": "^2.18.1",
    "pino": "^7.10.0"
  },
  "peerDependencies": {
    "fp-ts": "^2.11.5",
    "io-ts": "^2.2.16"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "husky": "^8.0.0",
    "io-ts-types": "^0.3.5",
    "jest": "^27.4.5",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.3"
  },
  "bugs": {
    "url": "https://github.com/macai-project/framework-node/issues"
